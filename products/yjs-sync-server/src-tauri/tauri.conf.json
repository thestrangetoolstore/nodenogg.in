{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "yjs-sync-server",
  "version": "0.1.0",
  "identifier": "in.nodenogg.websockets",
  "build": {
    "frontendDist": "../frontend",
    "devUrl": "ws://localhost:8787",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "@nodenogg.in/yjs-sync-server",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": false
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "../dist/*"
    ]
  }
}